// Basic variable declarations and access
var a = 1;
var b = 2;
var c = 3;
print a;
print b;
print c;

print (a + b) * c / a;
print a * b + c * a;

// Variable reassignments
a = 10;
b = 20;
c = 30;
print "HERE";
print a;
print b;
print c;

// Expressions with variables
print a + b;
print b - a;
print a * c;
print c / a;

// Chained assignments
print "HERE";
a = b = c;
print a;
print b;
print c;

// Multiple variables
var v1 = 100;
var v2 = 200;
var v3 = 300;
var v4 = 400;
var v5 = 500;
print v1;
print v2;
print v3;
print v4;
print v5;

// Long variable names
var thisIsAVeryLongVariableName = 42;
var anotherVeryLongVariableName = 84;
print thisIsAVeryLongVariableName;
print anotherVeryLongVariableName;
print thisIsAVeryLongVariableName + anotherVeryLongVariableName;

// Complex expressions
print "STOP";

print a;
print b;
print c;
print (a + b) * c / a;
print a * b + c * a;

// String variables
var greeting = "Hello";
var name = "World";
print greeting;
print name;
print greeting + ", " + name + "!";

// Variable reuse with different types
var reuse = 100;
print reuse;
reuse = "Now I'm a string";
print reuse;
reuse = 3.14159;
print reuse;

