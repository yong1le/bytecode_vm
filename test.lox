// and uses it to filter a list of numbers
fun makeFilter(min) {
  fun filter(n) {
    if (n < min) {
      return false;
    }
    return true;
  }
  return filter;
}

// This function applies a function to a list of numbers
fun applyToNumbers(f, count) {
  var n = 0;
  while (n < count) {
    if (f(n)) {
      print n;
    }
    n = n + 1;
  }
}

var greaterThanX = makeFilter(67);
print greaterThanX;
var greaterThanY = makeFilter(72);

print "Numbers >= 67:";
applyToNumbers(greaterThanX, 67 + 5);

print "Numbers >= 72:";
applyToNumbers(greaterThanY, 72 + 5);
